{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "label": "dev",
      "script": "dev",
      "isBackground": true,
      "problemMatcher": [
        {
          "owner": "webpack",
          "fileLocation": [
            "relative",
            "${workspaceFolder}"
          ],
          "pattern": {
            "regexp": "NEVER MATCH THIS"
          },
          "background": {
            "activeOnStart": true,
            "beginsPattern": "^\\[main\\] .*\\[built\\]$",
            "endsPattern": "^\\[main\\] webpack [0-9.]+ compiled successfully.*$"
          }
        }
      ]
    }
  ]
}

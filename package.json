{
  "name": "aristotl",
  "version": "0.3.0",
  "description": "An experimental logical fallacy lookup tool.",
  "homepage": "https://www.aristotl.io",
  "keywords": [
    "aristotl",
    "aristotle",
    "logical",
    "fallacy",
    "fallacies",
    "logic",
    "reasoning"
  ],
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production webpack --colors --profile --config config/build.conf.ts --display-error-details",
    "postbuild": "react-snap && ts-node config/build.post",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development webpack-dev-server --config config/build.conf.ts",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" \"config/**/*.ts\" \"config/**/*.tsx\" --config tslint.json --project tsconfig.json",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "reactSnap": {
    "source": "build",
    "include": [
      "/",
      "/preview",
      "/404.html"
    ],
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/chalk": "^2.2.0",
    "@types/cookie": "^0.4.0",
    "@types/copy-webpack-plugin": "^5.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/is-touch-device": "^1.0.0",
    "@types/lodash": "^4.14.151",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.0.3",
    "@types/node-polyglot": "^0.4.34",
    "@types/prismic-dom": "^2.1.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-hammerjs": "^1.0.1",
    "@types/react-hot-loader": "^4.1.1",
    "@types/react-redux": "^7.1.8",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-transition-group": "^4.2.4",
    "@types/require-dir": "^1.0.1",
    "@types/styled-components": "^5.1.0",
    "@types/webpack": "^4.41.13",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-env": "^1.15.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "chalk": "^4.0.0",
    "cookie": "^0.4.1",
    "copy-webpack-plugin": "^5.1.1",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "react-hot-loader": "^4.12.21",
    "react-snap": "^1.23.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.5.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "abortcontroller-polyfill": "^1.4.0",
    "body-scroll-lock": "^3.0.2",
    "core-js": "^3.6.5",
    "fuse.js": "^6.0.0",
    "html-react-parser": "^0.10.3",
    "is-touch-device": "^1.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "node-polyglot": "^2.4.0",
    "prismic-dom": "^2.2.3",
    "prismic-javascript": "^2.7.1",
    "promptu": "^2.6.0",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hammerjs": "^1.0.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "5.1.0",
    "whatwg-fetch": "^3.0.0"
  }
}
